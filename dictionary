word = input("Type your words: ")
ignored = "!@#$%^&*().,<>/?|~':;[]{}"
voc = []

word = word.split()

for x in word:
    for y in x:
        for z in ignored:
            if y == z:
                print("FUCK")
                x.replace(y, "")
    if len(x) > 3:
        voc.append(x)
voc.sort()
for i in voc:
    print(i, end="\n")
